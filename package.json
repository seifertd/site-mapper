    "@babel/register": "^7.15.3",
    "@commitlint/prompt": "^13.1.0",
    "async": "^2.6.3",
    "bunyan": "^1.8.15",
    "coffee-script": "^1.12.7",
    "csv": "^5.5.0",
    "iconv-lite": "^0.4.24",
    "lodash": "^4.17.21",
    "request": "^2.88.2",
    "sax": "^1.2.4",
