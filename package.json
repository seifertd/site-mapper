{
  "name": "site-mapper",
  "description": "sitemap.xml generation in node.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seifertd/site-mapper"
  },
  "author": {
    "name": "Douglas A. Seifert",
    "email": "doug@dseifert.net",
    "url": "http://www.dseifert.net"
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "commit": "git-cz",
    "prepublish": "npm start",
    "start": "babel src --out-dir lib",
    "test": "CONFIG_DIR=$PWD/test NODE_ENV=test mocha",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "main": "./lib/main.js",
  "bin": "./bin/sitemapper",
  "readmeFilename": "README.md",
  "keywords": [
    "es6",
    "es2015",
    "node",
    "module",
    "sitemap"
  ],
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "async": "^1.5.2",
    "bunyan": "^1.8.1",
    "coffee-script": "^1.10.0",
    "csv": "^0.4.0",
    "iconv-lite": "^0.4.13",
    "request": "^2.72.0",
    "sax": "^1.2.1",
    "stream-array": "^1.1.2",
    "stream-json": "^0.4.2",
    "underscore": "^1.8.3",
    "yargs": "^5.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.8.0",
    "chai": "^3.5.0",
    "commitizen": "^2.8.1",
    "concat-stream": "^1.5.1",
    "cz-conventional-changelog": "^1.1.6",
    "mocha": "^2.4.5",
    "semantic-release": "^4.3.5",
    "libxmljs": "^0.18.0"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
