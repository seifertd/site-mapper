    "libxmljs": "^0.19.5",
    "mocha": "^9.0.3",
    "semantic-release": "^17.4.5",
    "semantic-release-cli": "^4.1.2",
    "travis-deploy-once": "^3.3.0"
  },
  "config": {
    "commitizen": {
