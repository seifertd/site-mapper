{
  "name": "site-mapper",
  "description": "sitemap.xml generation in node.js.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seifertd/site-mapper"
  },
  "author": {
    "name": "Douglas A. Seifert",
    "email": "doug@dseifert.net",
    "url": "http://www.dseifert.net"
  },
  "scripts": {
    "clean": "rm -rf node_modules test/tmp/*",
