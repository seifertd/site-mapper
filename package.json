{
  "name": "site-mapper",
  "description": "sitemap.xml generation in node.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seifertd/site-mapper"
  },
  "author": {
    "name": "Douglas A. Seifert",
    "email": "doug@dseifert.net",
    "url": "http://www.dseifert.net"
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "commit": "git-cz",
    "prepublish": "npm start",
    "start": "babel src --out-dir lib",
    "test": "CONFIG_DIR=$PWD/test NODE_ENV=test mocha",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "main": "./lib/main.js",
  "bin": "./bin/sitemapper",
  "readmeFilename": "README.md",
  "keywords": [
    "es6",
    "es2015",
    "node",
    "module",
    "sitemap"
  ],
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "@commitlint/prompt": "^3.2.0",
    "async": "^2.5.0",
    "bunyan": "^1.8.12",
    "coffee-script": "^1.12.7",
    "csv": "^1.1.1",
    "iconv-lite": "^0.4.19",
    "lodash": "^4.17.4",
    "request": "^2.83.0",
    "sax": "^1.2.4",
    "stream-array": "^1.1.2",
    "stream-json": "^0.4.2",
    "yargs": "^7.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^3.2.0",
    "@commitlint/config-angular": "^3.1.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "commitizen": "^2.9.6",
    "concat-stream": "^1.6.0",
    "libxmljs": "^0.18.7",
    "mocha": "^2.5.3",
    "semantic-release": "^8.0.0",
    "semantic-release-cli": "^3.2.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  }
