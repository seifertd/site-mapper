{
  "name": "site-mapper",
  "description": "sitemap.xml generation in node.js.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seifertd/site-mapper.git"
  },
  "author": {
    "name": "Douglas A. Seifert",
    "email": "doug@dseifert.net",
    "url": "http://www.dseifert.net"
  },
  "scripts": {
    "clean": "rm -rf node_modules test/tmp/*",
    "commit": "git-cz",
    "lint": "eslint lib/ bin/sitemapper test/",
    "test": "rm -rf test/tmp/*; CONFIG_DIR=$PWD/test NODE_ENV=test mocha",
    "semantic-release": "semantic-release"
  },
  "main": "./lib/main.js",
  "bin": {
    "site-mapper": "bin/sitemapper"
  },
  "readmeFilename": "README.md",
  "keywords": [
    "es6",
    "es2015",
    "node",
    "module",
    "sitemap"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "csv": "^6.3.1",
    "pino": "^9.0.0",
    "sax": "^1.2.4",
    "stream-json": "^1.8.0",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-angular": "^18.1.0",
    "@commitlint/prompt": "^18.2.0",
    "@eslint/js": "^10.0.1",
    "chai": "^4.3.7",
    "commitizen": "^4.2.4",
    "concat-stream": "^2.0.0",
    "eslint": "^10.0.2",
    "libxmljs2": "^0.37.0",
    "mocha": "^10.2.0",
    "semantic-release": "^22.0.0",
    "semantic-release-cli": "^5.4.4"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "version": "4.0.0"
}
